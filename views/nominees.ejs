<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Nominees</title>
  <!-- Font Awesome -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
  <!-- Font Awesome -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
  <!-- MDB -->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.4.0/mdb.min.css" rel="stylesheet" />
  <link rel="stylesheet" href="style.css">
  <!-- MDB -->
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.4.0/mdb.min.js"></script>

</head>

<header>
  <!-- Sidebar -->
  <nav id="sidebarMenu" class="collapse d-lg-block sidebar collapse bg-white">
    
    <div class="position-sticky">
      <div class="list-group list-group-flush mx-3 mt-4">
        <a href="/contact-us" class="list-group-item list-group-item-action py-2 ripple" aria-current="true">
          <i class="fas  fa-fw me-3"></i><span>Contactors</span>
        </a>
        <a href="/subscribers" class="list-group-item list-group-item-action py-2 ripple ">
          <i class="fas  fa-fw me-3"></i><span>Subscribers</span>
        </a>
        <a href="/nominee" class="list-group-item list-group-item-action py-2 ripple ">
          <i class="fas f fa-fw me-3"></i><span>Nominees</span>
        </a>

      </div>
    </div>
  </nav>
  <!-- Sidebar -->

  <!-- Navbar -->
  <nav id="main-navbar" class="navbar navbar-expand-lg navbar-light bg-white fixed-top">
    <!-- Container wrapper -->
    <div class="container-fluid">
      <!-- Toggle button -->
      <button class="navbar-toggler" type="button" data-mdb-toggle="collapse" data-mdb-target="#sidebarMenu" aria-controls="sidebarMenu" aria-expanded="false" aria-label="Toggle navigation">
        <i class="fas fa-bars"></i>
      </button>

      <!-- Brand -->
      <a class="navbar-brand" href="#">
        <img src="logo-pink.png" height="45" alt="Zam Logo" loading="lazy" />
      </a>


      <!-- Avatar -->
      <!-- <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle hidden-arrow d-flex align-items-center" href="#" id="navbarDropdownMenuLink" role="button" data-mdb-toggle="dropdown" aria-expanded="false">
          <img src="logo-pink.png" class="rounded-circle" height="22" alt="Avatar" loading="lazy" />
        </a>
      </li> -->

    </div>
    <!-- Container wrapper -->
  </nav>
  <!-- Navbar -->

</header>

<body>

  <main>
    <%- include('dashboardNav') %>


    <div class="container">
      <h2>Nominees</h2>
    
      <table class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>ID No.</th>
            <th>Contact</th>
            <th>Address</th>
            <th>Highest Grade</th>
            <th>Motivation</th>
    
          </tr>
        </thead>
        <tbody id="myTable">
    
          <% for (var i = 0; i < data.length; i++) { %>
          <tr>
            <td class=""><%= data[i].name %></td>
            <td class=""><%= data[i].surname %></td>
            <td class=""><%= data[i].id_number %></td>
            <td class=""><%= data[i].numbers %></td>
            <td class=""><%= data[i].address %></td>
            <td class=""><%= data[i].grade %></td>
            <td class=""><%= data[i].message %></td>
    
          </tr>
          <% } %>
    
        </tbody>
      </table>
    
    </div>
  </main>



  
</body>

</html>